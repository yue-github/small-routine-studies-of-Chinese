@import "../template-navHead/template-navHead.wxss";
.componayMsg{
  width:100%;
  height:100rpx;
  box-sizing:border-box;
  display:flex;
  align-items: center;
}
.logo{
  width:50rpx;
  height:50rpx;
  margin-right:10rpx;
}
.cpmpony-name{
  font-size:40rpx;
}
.content-main{
  width:100%;
  box-sizing:border-box;
  padding:0 18rpx;
}
.content{
  width:100%;
  font-size:25rpx;
  display:flex;
  align-items: center;
  flex-direction: column;
