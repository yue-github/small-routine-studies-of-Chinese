.container{
  width:100%;
  box-sizing:border-box;
  overflow: hidden;
  background:#F0F0F0;
  font-size:30rpx;
}
.search{
  width:100%;
  box-sizing:border-box;
  display:flex;
  align-items: center;
  justify-content: center;
  padding:18rpx;
  background:#fff;
  color:#8E8E8E;
}
.search-font{
  padding:10rpx;
  text-align:center;
  width:91%;
  box-sizing:border-box;
  background:#F0F0F0;
  border-radius:5px;
  font-size:30rpx;
}
.font-small{
  font-size:28rpx;
  margin-left:10rpx;
}
.search-right{
  width:9%;
  display:flex;
  box-sizing:border-box;
  justify-content: flex-end;
  align-items:center;
  font-size:40rpx;
}
.join{
  width:100%;
  height:250rpx;
  
  background:#fff;
  box-sizing:border-box;
  padding:18rpx;
  display:flex;
  align-items: center;
  position:relative;
}
.road{
  width:220rpx;
  height:120rpx;
  position:absolute;
  bottom:18rpx;
  left:430rpx;
}
.join-img{
  width:200rpx;
  height:200rpx;
}
.join-font{
  margin-left:75rpx;
  padding:2rpx 10rpx;
  border-radius:10rpx;
  background:#55b7f5;
  font-size:25rpx;
  color:#fff;
  box-sizing:border-box;
}
.join-center{
  margin-left:50rpx;
}
.how-people{
  margin-top:35rpx;
  font-size:20rpx;
  color:#ccc;
}
.banner-swiper{
  width:100%;
  transform: translateZ(0);
}
.banner-swiper swiper-item{
  width:100%;
}
.banner-swiper swiper-item image{
  display:block;
  width:100%;
  height:350rpx;
}
.nav{
  width:100%;
  height:125rpx;
  padding:18rpx;
  background:#fff;
  display:flex;
  box-sizing:border-box;
  justify-content: space-between;
  align-items: center;
}
.nav-child{
  display:flex;
  flex-direction: column;
  align-items: center;
  
   
}
.nav-fa{
  font-size:50rpx;
  color:#55b7f5;
}
.nav-font{
  font-size:20rpx;
  margin-top:10rpx;
}
.column{
  width:100%;
  box-sizing:border-box;
  
  background:#fff;
  margin-top:18rpx;
  font-size:20rpx;
}
.column-main{
  width:100%;
  box-sizing:border-box;
  display:flex;
  justify-content: space-evenly;
  align-items: center;
  flex-wrap:wrap;
  padding:3rpx 0rpx;
}
.column-content{
  width:45%;
  display:flex;
  flex-direction: column;
  margin-bottom:35rpx;
  
}
.img-detail{
  width:340rpx;
  height:170rpx;
}
.column-img{
  position:relative;

}
.column-status{
  position:absolute;
  right:0;
  bottom:0;
  background:#55b7f5;
  padding:1rpx 10rpx;
  border-radius:10px 0px 0px 10px;
  color:#fff;
  box-sizing:border-box;
}
.column-title{
  width:100%;
  height:80rpx;
  box-sizing:border-box;
  padding:18rpx;
  display: flex;
  align-items: center;
  position:relative;
  margin-bottom:10rpx;
 
}
.special{
  display: flex;
  align-items: center;
  margin-left:30rpx;
  color:#ccc;
  letter-spacing: 5rpx;
}
.zhuanlan{
  font-size:25rpx;
}
.column-more{
  margin-left:320rpx;
  color:#8E8E8E;
}
.whoColumn{
  margin-top:8rpx;
}
.howPeople{
  color:#ccc;
  font-size:18rpx;
  margin-top:10rpx;
}
.class-msg{
  margin-top:23rpx;
}
.price{
    color:#f40;
    float:right;
    font-size:18rpx;
    margin-top:-33rpx;
}
.price-font{
  font-size:30rpx;
}
.culture{
  width:100%;
  background:#F0F0F0;
  margin-top:18rpx;
}
.culture-title{
    width:100%;
    height:80rpx;
    display:flex;
    
    align-items: center;
    font-size:20rpx;
    padding:18rpx;
    background:#fff;
    box-sizing:border-box;
}
.cultureSpecial{
  margin-left:30rpx;
  letter-spacing: 5rpx;
  color:#ccc;
}
.mingshi{
  font-size:25rpx;
}
.culture-more{
  margin-left: 275rpx;
}
.content-detail{
  box-sizing:border-box;
  padding:8rpx 10px;
  background:#fff;
}
.detail-title{
  display:flex;
  align-items:center;
  font-size:25rpx;
  color:#ccc;
}
.user-logo{
  width:50rpx;
  height:50rpx;
  border-radius:100%;
}
.user-name{
  margin:0 5px;

}
.content-title{
  margin:10px 0;
}
.user-img{
  width:100%;
  height:320rpx;
}
.love-detail{
  width:100%;
  display:flex;
  margin-top:5px;
  text-indent: 65rpx;
  text-align: left;
  font-size:29rpx;
  letter-spacing: 3rpx;
  color: #6b6666;
  background:#fff;
}
.changeFont{
  font-size:30rpx;
  color:#28FF28;
}
.changeSmall{
  font-size:26rpx;
  margin-right:50rpx;
  margin-left:8rpx;
}
.comment{
  margin:5px 0;
}
.content-detail{
  margin-top:10px;
}
.change-marginTop{
  margin:0;
  margin-bottom:13rpx;
}